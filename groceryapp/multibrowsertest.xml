<suite name="multibrowsertesting" parallel="tests" thread-count="2">
		
	<test name="Chrometest" thread-count="5">
	<parameter name="browser" value="Chrome"></parameter>
		<classes>
			<class name="testcases.ManageFooterTest">
				<methods>
					<include name="verifyUpdateFooterText"></include>
				</methods>
			</class>
		</classes>
	</test>

	<test name="edgetest" thread-count="5">
	<parameter name="browser" value="edge"></parameter>
		<classes>
			<class name="testcases.ManageFooterTest">
				<methods>
					<include name="verifyUpdateFooterText"></include>
				</methods>
			</class>
		</classes>
	</test>
</suite>
